{
  "rules": {
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }],
    "no-underscore-dangle": [ "error", {
      "allow": [ "_json" ]
    }]
  },
  "globals": {
    "document": true,
    "Event": true,
    "window": true
  },
  "env": {
      "jest": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint"
}

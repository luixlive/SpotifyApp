{
  "APP_DESCRIPTION": "Simple webapp to see your user's Spotify statistics.",
  "APP_TITLE": "Spotify Stats",
  "HOST": "localhost",
  "LOG_FILE": "./logs/spotifystats.log",
  "LOG_LEVEL": "debug",
  "PORT": 3000,
  "SESSION_VALUES": ["accessToken", "profile._json", "refreshToken"],
  "SPOTIFY_API_URL": "https://api.spotify.com/",
  "SPOTIFY_API_ENDPOINTS": {
    "TOP_ARTISTS": {
      "URI": "v1/me/top/artists",
      "OPTIONS": ["limit", "offset", "time_range"]
    }
  },
  "SPOTIFY_AUTHORIZATION_SCOPE": ["user-top-read"],
  "SPOTIFY_CALLBACK_URL": "localhost:3000/api/authentication/spotify/callback"
}
